CREATE database IF NOT EXISTS Proyecto_final;

USE Proyecto_final;

-- Table 'Categoria'

CREATE TABLE IF NOT EXISTS Categoria (
   id_Categoria INT NOT NULL primary KEY AUTO_INCREMENT,
   Nombre VARCHAR(80) NOT NULL);

-- Table 'Clientes_(usuarios)'

CREATE TABLE IF NOT EXISTS Clientes_usuarios(
  id_Clientes INT NOT NULL primary key AUTO_INCREMENT,
  Holder_name VARCHAR(100) NOT NULL,
  Edad INT NOT NULL,
  Dirección VARCHAR(80) NOT NULL,
  Telefono VARCHAR(25) NOT NULL,
  Email VARCHAR(80) NOT NULL);

-- Table 'Aliados'

CREATE TABLE IF NOT EXISTS Aliados (
  id_Store INT NOT NULL PRIMARY KEY AUTO_INCREMENT,
  Nombre_store VARCHAR(80) NOT NULL,
  id_cuenta_banc VARCHAR(80) NOT NULL,
  Dirección VARCHAR(80) NOT NULL,
  Telefono VARCHAR(25) NOT NULL,
  id_categoria INT NOT NULL,
  foreign key (id_categoria) references Categoria(id_categoria));

-- Table Productos

CREATE TABLE IF NOT EXISTS Productos(
  id_Producto INT NOT NULL PRIMARY KEY AUTO_INCREMENT,
  Nombre VARCHAR(80) NOT NULL,
  Marca VARCHAR(80) NOT NULL,
  Precio DECIMAL NOT NULL,
  id_categoria INT NOT NULL,
  foreign key (id_categoria) references Categoria(id_categoria),
  Id_store INT NOT NULL,
  foreign key (Id_store) references Aliados(Id_store));
 
-- Table Ventas

CREATE TABLE IF NOT EXISTS Ventas(
  id_Venta INT NOT NULL PRIMARY KEY AUTO_INCREMENT,
  Fecha_venta DATE NOT NULL,
  id_clientes INT NOT NULL,
  foreign key (id_clientes) references Clientes_usuarios(id_clientes),
  id_producto INT NOT NULL,
  foreign key (id_producto) references Productos(id_producto),
  valor_a_pagar decimal) ;
  
-- Table Matriz_ventas

CREATE TABLE IF NOT EXISTS Matriz_ventas (
  id_VentaXProducto INT NOT NULL primary key AUTO_INCREMENT,
  id_venta INT NOT NULL,
  foreign key (id_venta) references Ventas(id_venta),
  Comisión DECIMAL(8,2) NOT NULL,
  Impuestos DECIMAL(8,2) NOT NULL,
  valor_a_transferir_aliado DECIMAL NOT NULL);
  
  -- Table Matriz_pagos
  
CREATE TABLE IF NOT EXISTS Matriz_pagos(
  id_pago INT NOT NULL primary key AUTO_INCREMENT,
  Cuenta_pago VARCHAR(45) NOT NULL,
  Bank_reference VARCHAR (45) NOT NULL,
  id_VentaXProducto INT NOT NULL,
  foreign key (id_VentaXProducto) references Matriz_ventas(id_VentaXProducto),
  valor_a_transferir_aliado DECIMAL(8,0) NOT NULL);

-- Table Eventos

CREATE TABLE IF NOT EXISTS Eventos (
  id_evento INT NOT NULL primary key AUTO_INCREMENT,
  id_VentaXProducto INT NOT NULL,
  foreign key (id_VentaXProducto) references Matriz_ventas(id_VentaXProducto),
  Gateway VARCHAR(80) NOT NULL,
  Update_at DATETIME NOT NULL,
  Status VARCHAR(80) NOT NULL,
  Valor_a_pagar DECIMAL(8,2) NOT NULL,
  Event_type VARCHAR(80) NOT NULL);

-- Table Extracto

CREATE TABLE IF NOT EXISTS Extracto (
  id_movimiento INT NOT NULL PRIMARY KEY AUTO_INCREMENT,
  id_pago int ,
  foreign key (id_pago) references Matriz_pagos(id_pago),
  Cuenta_pago VARCHAR(45) NOT NULL,
  Bank_reference VARCHAR(45) NOT NULL,
  id_cuenta_banc VARCHAR(80) NOT NULL,
  Valor_a_pagar DECIMAL(8,2) NOT NULL,
  Saldo DECIMAL(10,2) NOT NULL);
  
  insert into categoria values 
(null,'Restaurantes'),
(null, 'CPGs'),
(null, 'Turbo'),
(null, 'PCR');

