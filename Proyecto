-- VISTAS --

use proyecto_final;

-- 1.
create view ventasxproducto as 
select id_producto, sum(valor_a_pagar) as Valor_a_pagar from ventas group by id_producto order by id_producto asc ;

select * from ventasxproducto;

-- 2.
CREATE OR REPLACE VIEW ventasxproductoxaliado as
select T1.id_producto,T2.Valor_a_pagar,T1.nombre,T1.id_store,T3.Nombre_store from productos T1
inner join ventasxproducto T2
inner join aliados T3
on T1.id_producto=T2.id_producto and T1.id_store=T3.id_store;

select * from ventasxproductoxaliado;

-- 3. 
CREATE OR REPLACE VIEW  ProductosxCategoria as
select id_store, Nombre_store,id_categoria from aliados where id_categoria = 1;

select * from productosxcategoria;

-- 4.
CREATE OR REPLACE VIEW ClientesXVentas as
select T1.id_venta, T2.valor_a_transferir_aliado,T3.Holder_name from ventas T1
inner join matriz_ventas T2
inner join clientes_usuarios T3 on
T1.id_Venta=T2.id_venta and T1.id_clientes=T3.id_Clientes;

select *from ClientesXVentas;
-- 5 

CREATE OR REPLACE VIEW ProductosXaliado
AS SELECT id_store , count(id_producto) AS 'total_produtos' FROM productos 
GROUP BY id_store;

select * from ProductosXaliado;

-- FUNCIONES--
-- 1. 
DELIMITER //
create function promedio(id_cl float) returns float
deterministic
begin
   declare respuesta float;
   If id_cl = 0 Then
   set respuesta= 'Ingrese numero mayor a 0';
   else 
   set respuesta= (select avg(valor_a_pagar) from ventas where id_cl=id_clientes);
   end if;
   return respuesta;
   end
   //
    
select promedio (8);

-- 2. 
DELIMITER //
create function Ventasxclientes(id int) returns float
deterministic
begin
   declare respuesta float;
   set respuesta = (select count(id_venta) from ventas where id=id_Clientes)  ;
   return respuesta;
   end
   //
   
select ventasxclientes (12);

-- STORED PROCEDURES--

-- 1. Con este proceso almacenado se podrá revisar si ya estan creados los aliados y si no estan hacer el registro en la tabla.

DELIMITER //
create procedure INS_ALI(Nom_str VARCHAR(80),
id_cnt_banc VARCHAR(80),
Dire VARCHAR(80),
Tel VARCHAR(80),
id_cat INT)
begin 
    DECLARE existe_aliado INT;
    DECLARE id INT;
    set existe_aliado = (select count(*) from aliados where Nom_str=Nombre_store);
    if existe_aliado=0 then 
          insert into aliados(Nombre_store,id_cuenta_banc,Dirección,Telefono,id_categoria)
          values(Nom_str,id_cnt_banc,Dire,Tel,id_cat);
		  set id=last_insert_id();
	else set id='0'; 
	end if ;
    select id;
end 
//

CALL INS_ALI('Itanos','57641254981','Calle 9 72-34','6013459010',1);

-- 2. 
/* Con este SP se podra ordenar la tabla ventas por cada una de las diferentes columnas que la componen*/

DELIMITER $$
CREATE PROCEDURE sp_order_ventas (IN v_ventas VARCHAR(100))
BEGIN
	IF v_ventas != '' THEN
		SET @ventas_order = CONCAT(' ORDER BY ' , v_ventas);
	ELSE
		SET @ventas_order = '';
	END IF;
    SET @clausula_ventas = CONCAT('SELECT * FROM ventas', @ventas_order);
    
    PREPARE runSQL FROM @clausula_ventas;
    EXECUTE runSQL;
    DEALLOCATE PREPARE runSQL;
    
END $$
DELIMITER ;

select * FROM ventas ORDER BY id_clientes;
CALL sp_order_ventas('id_clientes');
CALL sp_order_ventas('valor_a_pagar');
CALL sp_order_ventas('id_producto');


/* Con este SP se podra ordenar cualquier tabla de manera ascedente o descendente según 3 parametros que se le indiquen*/

DROP PROCEDURE IF EXISTS sp_order_tablas;

DELIMITER $$
CREATE PROCEDURE sp_order_tablas( IN campo VARCHAR(255) , IN type_order ENUM('ASC' ,'DESC',''), IN tabla VARCHAR(200) )
BEGIN
    
    IF campo != '' THEN
		SET @orden = CONCAT(' ORDER BY ' , campo);
	ELSE
		SET @orden = '';
	END IF;
    
    IF type_order != '' THEN
		SET @tipo = CONCAT(' ' , type_order); -- ' DESC'
	ELSE
		SET @tipo = '';
	END IF;
    
    SET @clausula_tabla = CONCAT('SELECT * FROM ', tabla ,@orden,@tipo);
    
    PREPARE runSQL FROM @clausula_tabla;
    EXECUTE runSQL;
    DEALLOCATE PREPARE runSQL;
    
END $$
DELIMITER ;

select * from productos ORDER BY NOMBRE desc;
CALL sp_order_tablas('Nombre' ,'DESC', 'productos');

CALL sp_order_tablas('Nombre_store' ,'DESC', 'aliados');

CALL sp_order_tablas('edad' ,'', 'clientes_usuarios');


/*TRIGGERS*/

/*Tabla espejo de tabla_aliados*/

CREATE TABLE IF NOT EXISTS aliados_back_up (
  id_Store INT NOT NULL PRIMARY KEY AUTO_INCREMENT,
  Nombre_store VARCHAR(80) NOT NULL,
  id_cuenta_banc VARCHAR(80) NOT NULL,
  Dirección VARCHAR(80) NOT NULL,
  Telefono VARCHAR(25) NOT NULL,
  id_categoria INT NOT NULL,
  foreign key (id_categoria) references Categoria(id_categoria));
  
  /*Aplico AFTER e INSERT*/

CREATE TRIGGER back_up_tabla_aliados
AFTER INSERT ON aliados 
FOR EACH ROW 
INSERT INTO aliados_back_up(id_Store,Nombre_store,id_cuenta_banc,Dirección,Telefono,id_categoria)
VALUES(NEW.id_Store,NEW.Nombre_store,NEW.id_cuenta_banc,NEW.Dirección,NEW.Telefono,NEW.id_categoria);

/*Probamos el trigger de tabla espejo en ALIADOS */

INSERT INTO aliados(Nombre_store,id_cuenta_banc,Dirección,Telefono,id_categoria) VALUES('FDS','865214597','DirPrueba','3205563698','2');

SELECT * FROM aliados;
SELECT * FROM aliados_back_up;
DELETE FROM aliados WHERE id_Store='1048';
DELETE FROM aliados_back_up WHERE id_Store='1048';


/*Tabla espejo de Ventas
Aplico AFTER e INSERT*/

CREATE TABLE IF NOT EXISTS Ventas_back_up(
  id_Venta INT NOT NULL PRIMARY KEY AUTO_INCREMENT,
  Fecha_venta DATE NOT NULL,
  id_clientes INT NOT NULL,
  foreign key (id_clientes) references Clientes_usuarios(id_clientes),
  id_producto INT NOT NULL,
  foreign key (id_producto) references Productos(id_producto),
  valor_a_pagar decimal) ;

CREATE TRIGGER back_up_ventas 
AFTER INSERT ON ventas
FOR EACH ROW 
INSERT INTO Ventas_back_up(id_Venta,Fecha_venta,id_clientes,id_producto,valor_a_pagar)
VALUES(NEW.id_Venta,NEW.Fecha_venta,NEW.id_clientes,NEW.id_producto,NEW.valor_a_pagar);

/*Probamos el trigger de tabla espejo en VENTAS*/

INSERT INTO ventas(Fecha_venta,id_clientes,id_producto,valor_a_pagar) VALUES('2022-10-03',4,4008,7500);

select * from ventas;
select * from ventas_back_up;

/*Tabla bitacora al actualizar tabla Aliados
Aplico BEFORE y UPDATE*/

CREATE TABLE aliados_bitacora(
id_bitacora INT NOT NULL AUTO_INCREMENT,
fecha DATE NOT NULL,
hora TIME NOT NULL,
usuario_id VARCHAR(45),
old_id_cuenta_banc VARCHAR(80),
new_id_cuenta_banc VARCHAR(80),
old_Dirección VARCHAR(80),
new_Dirección VARCHAR(80),
tipo_operacion varchar(100),
primary key(id_bitacora));

CREATE TRIGGER update_log_aliados
BEFORE UPDATE ON aliados
FOR EACH ROW
INSERT INTO aliados_bitacora
(fecha,hora,usuario_id,old_id_cuenta_banc,new_id_cuenta_banc,old_Dirección,new_Dirección,tipo_operacion)
VALUES
(curdate(),curtime(),session_user(),OLD.id_cuenta_banc,NEW.id_cuenta_banc,OLD.Dirección,NEW.Dirección,'Actualizacion');

/*Probamos el trigger de tabla bitácora en ALIADOS*/

update aliados set Dirección='Calle 7 1-24' where id_Store='1003';
select * from aliados;
select * from aliados_bitacora;


/*Tabla bitacora al eliminar un registro en la tabla Eventos
Aplico BEFORE y DELETE*/

CREATE TABLE eventos_bitacora(
id_bitacora INT NOT NULL AUTO_INCREMENT,
fecha DATE NOT NULL,
hora TIME NOT NULL,
usuario_id VARCHAR(45),
old_status VARCHAR(80),
old_Event_type VARCHAR(80),
tipo_operacion varchar(100),
primary key(id_bitacora));

CREATE TRIGGER delete_log_tabla_eventos
BEFORE DELETE ON eventos
FOR EACH ROW
INSERT INTO eventos_bitacora
(fecha,hora,usuario_id,old_status,old_Event_type,tipo_operacion)
VALUES
(curdate(),curtime(),session_user(),OLD.status,OLD.Event_type,'Delete');

/*Probamos el trigger de tabla bitácora en Eventos*/

INSERT INTO eventos (id_VentaXProducto,Gateway,Update_at,Status,Valor_a_pagar,Event_type) VALUES (2120,'SCOTIABANK','2022-10-16',2,120000.00,'PAYMENT');
DELETE FROM eventos WHERE id_VentaXProducto=2120;

select * from eventos;
select * from eventos_bitacora;




